{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 100,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^react", "^next", "^@?\\w", "^@/", "^../", "^./", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
