{
  "name": "fresh",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "20.19.4",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "preinstall": "node scripts/check-node-version.cjs",
    "dev": "concurrently -k -n api,web -c blue,green \"pnpm dev:api\" \"pnpm dev:web\"",
    "dev:web": "pnpm --filter @apps/web dev",
    "dev:api": "pnpm --filter @services/api dev",
    "dev:check-node": "node scripts/check-node-version.cjs",
    "dev:restart": "./scripts/restart-dev.sh",
    "dev:kill": "./scripts/kill-dev-processes.sh",
    "dev:status": "./scripts/dev-status.sh",
    "typecheck": "pnpm -r --parallel --if-present typecheck",
    "lint": "pnpm -r --parallel --if-present lint",
    "lint:fix": "pnpm -r --parallel --if-present lint:fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "pnpm --filter @packages/types build && pnpm -r --parallel --if-present build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lighthouse": "lhci autorun",
    "analyze": "cd apps/web && ANALYZE=true pnpm build",
    "start:web": "cd apps/web && pnpm start",
    "perf:test": "pnpm build && pnpm start:web & sleep 10 && pnpm lighthouse",
    "bundle:analyze": "cd apps/web && ANALYZE=true pnpm build",
    "bundle:watch": "bundlewatch",
    "docs": "typedoc --out docs/API packages",
    "doctor": "./scripts/doctor.sh",
    "prepare": "husky install",
    "commit:skip-hooks": "HUSKY=0 git commit -m \"chore: commit without hooks (temporary)\"",
    "gen:icons": "tsx scripts/generate-icons.ts"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.0.1",
    "eslint": "^9.9.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "semver": "^7.6.3",
    "server-only": "^0.0.1",
    "supertest": "^7.1.4",
    "typedoc": "^0.28.12",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2",
    "sharp": "^0.33.4"
  },
  "dependencies": {
    "firebase": "^12.2.1",
    "next": "15.5.2"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
