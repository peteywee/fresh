{
  "name": "@packages/types",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": "20.19.4",
    "pnpm": ">=9.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./onboarding": {
      "types": "./dist/onboarding.d.ts",
      "import": "./dist/onboarding.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "zod": "3.23.8"
  },
  "devDependencies": {
    "typescript": "5.5.4"
  }
}
